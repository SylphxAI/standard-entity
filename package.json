{
	"name": "@sylphx/standard-entity",
	"version": "0.1.0",
	"description": "Standard Entity Protocol - Cross-library type-safe entity operations",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"typecheck": "tsc --noEmit",
		"lint": "biome check .",
		"format": "biome format --write .",
		"prepublishOnly": "bunx @sylphx/doctor prepublish",
		"test": "bun test"
	},
	"keywords": [
		"standard-entity",
		"entity",
		"protocol",
		"type-safe",
		"structural-typing",
		"cross-library"
	],
	"author": "Sylphx",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SylphxAI/standard-entity.git"
	},
	"homepage": "https://github.com/SylphxAI/standard-entity",
	"bugs": {
		"url": "https://github.com/SylphxAI/standard-entity/issues"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@sylphx/biome-config": "^0.4.0",
		"@sylphx/bump": "^1.4.11",
		"@sylphx/doctor": "^1.26.0",
		"@sylphx/tsconfig": "^0.3.0",
		"@types/bun": "^1.2.17",
		"lefthook": "^2.0.4",
		"typescript": "^5.8.3"
	}
}
